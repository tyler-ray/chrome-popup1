{
    "manifest_version":2,
    "name":"Popup extension",
    "version":"1.0.0",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["content.js"]
        }
    ],
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup": "popup.html",
        "default_title":"A popup will come here."
    }
}